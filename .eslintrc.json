{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
  "plugins": ["prettier", "jest"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "import/extensions": "off"
  }
}
